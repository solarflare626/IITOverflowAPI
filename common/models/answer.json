{
  "name": "Answer",
  "plural": "Answers",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "Counts": true
  },
  "properties": {
    "id": {
      "type": "number",
      "required": true
    },
    "answer": {
      "type": "text",
      "required": true
    },
    "createdAt": {
      "type": "date",
      "required": true,
      "defaultFn": "now"
    },
    "updatedAt": {
      "type": "date",
      "required": true,
      "defaultFn": "now"
    },
    "deletedAt": {
      "type": "date"
    }
  },
  "validations": [],
  "relations": {
    "user": {
      "type": "belongsTo",
      "model": "user",
      "foreignKey": "userId"
    },
    "question": {
      "type": "belongsTo",
      "model": "Question",
      "foreignKey": "questionId"
    }
  },
  "acls": [],
  "methods": {}
}
